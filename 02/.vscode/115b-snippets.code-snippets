  "Create nested pop up page": {
		"scope": "html",
		"prefix": "cxNestedPopup",
		"body": [
      "<html>",
      "<head>",
      "</head>",
      "<body>",
      "    <div class=\"pageWrapper\" id=\"recap\">",
      "        <!-- Text and Image -->",
      "        <div id=\"txt_open_close\" class=\"txt_close txt_open\"></div>",
      "        <div class=\"video_right_txt\">",
      "            <div class=\"content_scrol_box\">",
      "                <div class=\"heading-video-Page-title\">Identification</div>",
      "                <div class=\"video-rt-text-scroll\">",
      "                    <p class=\"para-p\">The cold section module has the following cornponents: </p>",
      "                    <ul class=\"list_style\">",
      "                        <li>Fan rotor assembly and A-sump components </li>",
      "                        <li>Fan stator </li>",
      "                        <li>Front frame cornoonents </li>",
      "                        <li>Power takeoff assembly </li>",
      "                        <li>IGVs and Stage 1 thru 4 variable vanes and stage 4 thru 9 fixed cornpressor stator vanes",
      "                        </li>",
      "                        <li>Compressor rotor components </li>",
      "                        <li>Combustion chamber and B-sump frame </li>",
      "                        <li>Fan drive shaft </li>",
      "                    </ul>",
      "                </div>",
      "            </div>",
      "        </div>",
      "        <div class=\"player_box_pin  bg-white\">",
      "            <div class=\"common_table\">",
      "                <div class=\"common_table_Cell\" id=\"\">",
      "                    <div id=\"cbtCia\" class=\"hotspot_box\">",
      "                        <img src=\"assets/image/m001/t01_m01_p10.jpg\">",
